{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${s}/out/**/*.js"
			],
			"preLaunchTask": "npm: watch"
		}
	],
	"logging": {
        "trace": true,
        "traceResponse": true,
		"engineLogging": true,
		"exceptions": true,
		"moduleLoad": true,
		"programOutput": true
    }
}
